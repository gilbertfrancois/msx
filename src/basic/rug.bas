100 ' Dots
110 ' Gilbert Francois Duivesteijn
120 '
130 PI=4*ATN(1)
200 COLOR 1,15,15
210 CLS
220 SCREEN 2
225 BS=8
226 BH=BS/2
227 CX=256/BS
228 CY=192/BS
230 FOR I=(32/BS-1) TO (CX-32/BS-1)
240 FOR J=2 TO CY-3
250 R = BH+BH*COS(I/CY*2*PI)*SIN(J/CY*2*PI)
260 CIRCLE(I*BS+BH,J*BS+BH),R*2,1
261 'PAINT(I*BS+BH,J*BS+BH),1
265 ' PRINT I,J,R
270 NEXT J
280 NEXT I
290 GOTO 290
